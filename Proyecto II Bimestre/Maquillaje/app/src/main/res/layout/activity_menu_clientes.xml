<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".modulo_clientes.MenuClientes" android:background="@android:color/white">
    <ImageView
            android:layout_width="163dp"
            android:layout_height="92dp" app:srcCompat="@drawable/maquillaje"
            android:id="@+id/imageView2" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"/>
    <TextView
            android:text="CATÁLOGO MAQUILLAJE"
            android:layout_width="277dp"
            android:layout_height="37dp"
            android:id="@+id/text_titulo" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView2"
            android:textSize="24sp" android:textColor="#817C7C" android:textAlignment="center"
            android:textStyle="bold"/>

    <Button
            android:text="@string/ver"
            android:layout_width="150dp"
            android:layout_height="60dp"
            android:id="@+id/btn_ver_cli"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="30dp"
            android:background="@drawable/round" android:textColor="#F5E9E3E3" android:textSize="18sp"
            app:layout_constraintStart_toEndOf="@+id/imageView12"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="1.0" android:layout_marginTop="30dp"
            app:layout_constraintTop_toBottomOf="@+id/button" android:layout_marginBottom="20dp"
            app:layout_constraintBottom_toTopOf="@+id/btn_ing_cli"/>
    <Button
            android:text="@string/modificarEliminar"
            android:layout_width="150dp"
            android:layout_height="60dp"
            android:id="@+id/btn_mod_cli"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="30dp"
            android:layout_marginBottom="20dp"
            android:background="@drawable/round" android:textSize="18sp" android:textColor="#F5E9E3E3"
            app:layout_constraintStart_toEndOf="@+id/imageView13" android:layout_marginStart="8dp"
            app:layout_constraintHorizontal_bias="1.0" android:layout_marginTop="30dp"
            app:layout_constraintTop_toBottomOf="@+id/btn_ing_cli"/>
    <Button
            android:text="@string/insertar"
            android:layout_width="150dp"
            android:layout_height="60dp"
            android:id="@+id/btn_ing_cli"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="30dp"
            android:background="@drawable/round"
            android:textColor="#F5E9E3E3" android:textSize="18sp"
            app:layout_constraintStart_toEndOf="@+id/imageView11" android:layout_marginStart="8dp"
            app:layout_constraintHorizontal_bias="1.0" android:layout_marginTop="30dp"
            app:layout_constraintTop_toBottomOf="@+id/btn_ver_cli" android:layout_marginBottom="20dp"
            app:layout_constraintBottom_toTopOf="@+id/btn_mod_cli"/>
    <ImageView
            android:layout_width="144dp"
            android:layout_height="89dp" app:srcCompat="@drawable/client"
            android:id="@+id/imageView11"
            app:layout_constraintTop_toTopOf="@+id/btn_ing_cli"
            app:layout_constraintBottom_toBottomOf="@+id/btn_ing_cli" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="20dp"/>
    <ImageView
            android:layout_width="148dp"
            android:layout_height="96dp" app:srcCompat="@drawable/ver" android:id="@+id/imageView12"
            app:layout_constraintTop_toTopOf="@+id/btn_ver_cli"
            app:layout_constraintBottom_toBottomOf="@+id/btn_ver_cli"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="20dp"/>
    <ImageView
            android:layout_width="144dp"
            android:layout_height="74dp" app:srcCompat="@drawable/editusu"
            android:id="@+id/imageView13"
            app:layout_constraintBottom_toBottomOf="@+id/btn_mod_cli"
            app:layout_constraintTop_toTopOf="@+id/btn_mod_cli" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="20dp"/>
    <Button
            android:text="Gestión Clientes"
            android:layout_width="388dp"
            android:layout_height="59dp"
            android:id="@+id/button"
            app:layout_constraintTop_toBottomOf="@+id/text_titulo" android:background="@drawable/img1"
            android:textColor="@android:color/white" android:textSize="30sp" android:textAllCaps="true"
            android:textStyle="bold" android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.0"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.333"/>
</android.support.constraint.ConstraintLayout>